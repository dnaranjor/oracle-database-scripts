/* SCRIPT PARA FILTRAR REGISTROS DE AUDITORIA POR ULTIMOS XX REGISTROS O POR FECHA */

SELECT * FROM DBA_AUDIT_TRAIL WHERE ROWNUM <=12

SELECT * FROM DBA_AUDIT_TRAIL WHERE TIMESTAMP >=TO_DATE ('01-05-2017 00:00','dd-MM-yyyy HH24:MI')

SELECT USERNAME
      , TERMINAL
      , TIMESTAMP
      , OBJ_NAME
      , COMMENT_TEXT  
FROM DBA_AUDIT_TRAIL
WHERE TIMESTAMP >= TO_DATE ('01-05-2017 00:00','dd-MM-yyyy HH24:MI')

